package edu.caltech.cs141b.hw2.gwt.collab.shared;
import edu.caltech.cs141b.hw2.gwt.collab.shared.Message.Type;

public class DocumentMessage extends Message {
	private UserValue user;
	private String status;
	
	@SuppressWarnings({"UnusedDeclaration"})
	// For GWT RPC
	private DocumentMessage() {
		super(Type.DOC_KEY);
	}
	
	public DocumentMessage(UserValue user, String status) {
		super(Type.DOC_KEY);
		this.user = user;
		this.status = status;
	}

	public UserValue getUser() {
		return user;
	}
	
	public String getStatus() {
		return status;
	}
  
}
