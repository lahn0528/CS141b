package edu.caltech.cs141b.hw2.gwt.collab.shared;

import java.io.Serializable;
import java.util.Date;

/**
 * Information containing login results
 *
 *
 */
public class LoginResults implements Serializable {
  private String channelId;
  private String userId;

  // For GWT RPC
  private LoginResults() {
  }

  public LoginResults(String userId, String channelId) {
    this.userId = userId;
    this.channelId = channelId;
  }

  public String getUserId() {
    return userId;
  }

  public String getChannelId() {
    return channelId;
  }

}

